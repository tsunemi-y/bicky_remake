{
    "compilerOptions": {
        "outDir": "./built/",
        "sourceMap": true,
        "strict": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "module": "es2015",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "moduleResolution": "node",
        "jsx": "react-jsx",
        "allowSyntheticDefaultImports": true,
        "target": "es6",
        "lib": [
            "es2016",
            "dom"
        ]
    },
    "include": [
        "resources/admin/ts/**/*" // TypeScriptのソース配置場所
, "resources/ts/admin/components", "resources/js/admin/app.js"    ]
}